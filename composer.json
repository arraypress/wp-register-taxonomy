{
  "name": "arraypress/wp-register-taxonomy",
  "version": "1.0.0",
  "description": "Lightweight library for registering taxonomies in WordPress with smart defaults, automatic label generation, and support for custom objects beyond post types.",
  "homepage": "https://github.com/arraypress/wp-register-taxonomy/",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\WP\\RegisterTaxonomy\\": "src/"
    },
    "files": [
      "src/Functions.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-register-taxonomy/issues",
    "source": "https://github.com/arraypress/wp-register-taxonomy"
  },
  "keywords": [
    "wordpress",
    "taxonomy",
    "taxonomies",
    "register",
    "custom-taxonomy",
    "categories",
    "tags",
    "terms",
    "permalink",
    "rewrite",
    "labels",
    "admin",
    "custom-objects"
  ]
}